# Traffic Class 0: (Protocol 6(TCP), Port 80) OR Dest IP 8.8.8.8 "Priority 2"
queueId: 0
maxPackets: 20
quantumSize: 500
isDefault: false
Filter: 
    DestPortNumber: 80
    ProtocolNumber: 6
    END_FILTER
Filter:
    DestIPAddress: 8.8.8.8
    END_FILTER
---

